{
  "version": 2,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/catalog.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/catalog.html",
      "destination": "/catalog.html?v=force"
    }
  ]
}
