{
  "version": 2,
  "cleanUrls": false,
  "headers": [
    {
      "source": "/(.*)\\.(html)$",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css)$",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/catalog.html",
      "destination": "/catalog-clean.html",
      "permanent": true
    },
    {
      "source": "/catalog-boutique.html",
      "destination": "/catalog-clean.html",
      "permanent": true
    }
  ]
}
