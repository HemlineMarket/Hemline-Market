{
  "version": 2,
  "cleanUrls": true,
  "redirects": [
    { "source": "/catalog", "destination": "/catalog.html", "permanent": false }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(html)$",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css)$",
      "headers": [
        { "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" },
        { "key": "Pragma", "value": "no-cache" }
      ]
    }
  ]
}
