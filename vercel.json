{
  "rewrites": [
    {
      "source": "/sitemap.xml",
      "destination": "/api/sitemap"
    },
    {
      "source": "/l/:id",
      "destination": "/api/listing/:id"
    },
    {
      "source": "/fabric/:id",
      "destination": "/api/listing/:id"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/process-orders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/check-saved-searches",
      "schedule": "0 */6 * * *"
    }
  ]
}
